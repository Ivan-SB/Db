{
    "name": "horde/db",
    "description": "Horde Database Libraries",
    "type": "library",
    "homepage": "https://pear.horde.org",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Mike Naberezny",
            "email": "mike@naberezny.com",
            "role": "lead"
        },
        {
            "name": "Chuck Hagenbuch",
            "email": "chuck@horde.org",
            "role": "lead"
        },
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.4.1",
    "time": "2017-10-20",
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3 || ^7",
        "pear-pear.horde.org/Horde_Date": "^2",
        "pear-pear.horde.org/Horde_Exception": "^2",
        "pear-pear.horde.org/Horde_Support": "^2",
        "pear-pear.horde.org/Horde_Util": "^2"
    },
    "suggest": {
        "pear-pear.horde.org/Horde_Autoloader": "^2",
        "pear-pear.horde.org/Horde_Cache": "^2",
        "pear-pear.horde.org/Horde_Log": "^2",
        "pear-pear.horde.org/Horde_Test": "^2.1",
        "ext-mysql": "*",
        "ext-mysqli": "*",
        "ext-oci8": "*",
        "ext-PDO": "*"
